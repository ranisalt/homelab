[Container]
ContainerName=%N
Environment=POSTGRES_DB=immich
Environment=POSTGRES_INITDB_ARGS=--data-checksums
Image=ghcr.io/immich-app/postgres@sha256:32324a2f41df5de9efe1af166b7008c3f55646f8d0e00d9550c16c9822366b4a
Pod=immich.pod
ShmSize=128mb
Volume=%D/immich/postgres:/var/lib/postgresql/data

[Service]
Restart=always

[Install]
WantedBy=default.target
