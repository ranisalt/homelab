[Container]
ContainerName=%N
Environment=POSTGRES_DB=immich
Environment=POSTGRES_INITDB_ARGS=--data-checksums
Image=ghcr.io/immich-app/postgres@sha256:c44be5f2871c59362966d71eab4268170eb6f5653c0e6170184e72b38ffdf107
Pod=immich.pod
ShmSize=128mb
Volume=%D/immich/postgres:/var/lib/postgresql/data

[Service]
Restart=always

[Install]
WantedBy=default.target
